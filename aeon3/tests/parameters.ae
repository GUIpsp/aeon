define test : (x:Integer) -> Integer {
    x * 10;
}

define aplicaFuncao : (f:(Integer -> Integer)) -> z:Integer {
    print(f(9));
}

define fatorial : (x:Integer) -> Integer {
    x == 0 ? 1 : x * fatorial(x - 1);
}

define fibonacci : ({x:Integer | x > 0}) -> Integer {
    x == 0 || x == 1 ? x : fibonacci(x - 1) + fibonacci(x - 2);
}

define main : () -> Void {
    print("Result of applying lambda function:");
    aplicaFuncao(\x:Integer -> x + 1);

    print("Result of applying a function:");
    aplicaFuncao(test);
    
    print("Result of fatorial:");
    print(fatorial(5));
    
    print("Result of applying fibonacci:");
    print(fibonacci(2));
}
