import ..prelude.J
    
safezero : (i:Integer) -> out:Integer where [out != 0] {
    r !: {Integer where self != 0} = J.iif(i == 0, () -> 1, () -> i)
    r
}

three : (i:Integer) -> out:Integer where [ out == (3*i) ] {
    a : {Integer where self != 0} = i
    3*i
}

main : (_:Array<String>) -> _:Void {
   J.out(three(3))
}
