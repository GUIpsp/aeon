// if n > 2000 (and nothing in 1000 < n < 2000)

small : String = "small"
large : String = "large"
none : String = ""

smaller_or_greater(x:Integer) -> out:String where {out == (x < 1000 ? ("small") : (x > 2000 ? "large" : ""))} {
    // x < 1000 ? (small) : (x > 2000 ? large : none);
    ??;
}

main() -> Top {
    print<String>(smaller_or_greater(1200));
