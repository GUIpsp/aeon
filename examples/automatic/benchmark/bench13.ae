import aeon/libraries/list;

// Vector Average:
// ---------------
// Given a vector of floats, return the average of those floats.
// Results are rounded to 4 decimal places.
vector_average({l:List<Double> | l.size > 0)} -> out:Double -> {} {
    reduce<Integer, Integer>(\x:Integer -> \y:Integer -> x + y, l) / length(l);
    // [Double]
}