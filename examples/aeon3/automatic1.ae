decipher(x:Integer, k:Integer) -> out:Integer {
    (x - k);
}

isValid(z:Integer) -> Boolean {
    z > 0 ? true : false;
}

cipher({x:Integer | x > 0}, key:Integer) -> z:Integer where {z > 0 and
                                                            x == decipher(z, key) and
                                                            isValid(z)} {
    [Integer];
}