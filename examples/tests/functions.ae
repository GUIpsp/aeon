f : () -> _:Void {}
	
f1 : () -> _:Integer { 1 }
	
f2 : () -> _:Double { 1.0 }

f3 : (x:Integer) -> _:Integer { x + 1 }

f4 : (x:Double) -> _:Double { x + 1 }

f5 : (x:Integer, y:Integer) -> _:Integer { x + y }

f6 : (x:Integer, y:Integer) -> _:Double { 1.0 * y }

main : (args:Array<String>) -> _:Void {
	f()
	f1()
	f2()
	f3(1)
	f4(1.0)
	f5(1, 1)
	f6(2, 1)
}
