import aeon/libraries/list;

// TODO: Bug, unable to check uninterpreted functions

main() -> Top {

    // Create an empty list
    print[String]("Empty list");
    l1 : List[Integer] = empty_list[Integer]();
    print[Integer](l1);

    // Append to the list
    print[String]("Apppend to list");
    l2 : List[Integer] = append[Integer](1, l1);
    print[Integer](l2);
    l2 = append[Integer](2, l2);
    print[Integer](l2);
    l2 = append[Integer](3, l2);
    print[Integer](l2);

    // Extend a list with another list
    print[String]("Extend a list with another list");
    l4 : List[Integer] = append[Integer](1, empty_list[Integer]());
    l5 : List[Integer] = append[Integer](2, empty_list[Integer]());
    print[Integer](extend[Integer](l1, l2)); // 0 items with + items
    print[Integer](extend[Integer](l2, l1)); // + items with 0 items
    print[Integer](extend[Integer](l4, l5)); // 1 item with 1 item
    print[Integer](extend[Integer](l5, l4)); // 1 item with 1 item
    print[Integer](extend[Integer](l2, l4)); // + items with 1 item
    print[Integer](extend[Integer](l4, l2)); // 1 item with + items

    // Insert at a specific place
    print[String]("Insert in a list");
    print[Integer](insert[Integer](0, l2, 0));
    print[Integer](insert[Integer](4, l2, 3));
    // print[Integer](insert[Integer](5000, l2, 5000)); // TODO: Should give error

    // Remove the first occurence
    print[String]("Remove an occurence");
    print[Integer](remove[Integer](-1, l2));
    print[Integer](remove[Integer](1, l2));
    print[Integer](remove[Integer](2, l2));
    print[Integer](remove[Integer](3, l2));

    // Check if list contains element
    print[String]("Contains an element");
    print[Integer](contains[Integer](-1, l2));
    print[Integer](contains[Integer](2, l2));

    // Count the amount of times an element occur
    print[String]("Count the times of element");
    print[Integer](count[Integer](-1, l2));
    print[Integer](count[Integer](1, l2));

    // Reverse a list
    print[String]("Reversing lists");
    print[Integer](reverse[Integer](empty_list[Integer]()));
    print[Integer](reverse[Integer](l2));

    // Exists
    print[String]("Exists in lists");
    f : (Integer -> Boolean) = \x: Integer -> x > 2;
    print[Integer](exists[Integer](f, l2));

    // Forall    
    print[String]("Forall in lists");
    f : (Integer -> Boolean) = \x: Integer -> x > 2;
    print[Integer](forall[Integer](f, l2));

    // Filter to get non-empty and empty list
    print[String]("Filter in lists");
    f : (Integer -> Boolean) = \x: Integer -> x > 2;
    f2 : (Integer -> Boolean) = \x: Integer -> x > 10;
    print[Integer](filter[Integer](f, l2));
    print[Integer](filter[Integer](f2, l2));

    // Map
    print[String]("Map in lists");
    f : (Integer -> Integer) = \x: Integer -> x * 2;
    f2 : (Integer -> Integer) = \x: Integer -> -x;
    print[Integer](map[Integer](f, l2));
    print[Integer](map[Integer](f2, l2));
}
