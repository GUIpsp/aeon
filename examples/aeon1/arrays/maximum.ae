import ..prelude.J
import ..prelude.A


max : (i:Integer, j:Integer) -> k:Integer { # where [ k >= i and k >= j ]
  J.iif(i > j, () -> i, () -> j)
} 

maximum : (ar:A.Array<Integer>) -> i:Integer {
  A.reduce(ar, (i:Integer, j:Integer) -> max(i,j))
} 

main : (args:Array<String>) -> _:Void { 
   a = A.range(0,1000)
   f = maximum(a)
   J.out(f)
}